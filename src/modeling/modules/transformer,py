import torch
from torch import nn
import torch.functional as F


class TransformerDecoder(nn.Module):
    def __init__(
            self,
            num_heads: int,
            embed_dim: int,
            key_dim: int,
            ff_dim: int,
            dropout_rate: float,
            ):
        super().__init__()
        self.num_heads = num_heads
        self.embed_dim = embed_dim
        self.key_dim = key_dim
        self.ff_dim = ff_dim
        self.dropout_rate = dropout_rate
        
       